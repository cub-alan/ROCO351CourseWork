function episode = runEpisode(qtable,state,action,next_state,reward)

improt initQ.*
import nextState.*
import reward.*
import randomStartingState.*
import epsilonGreedyAction.*
import update.*

initQ()
randomStartingState();
while (state~=2)

    epsilonGreedyAction(qtable,state);
    reward(state,action);
    update(qtable,state,action,next_state,reward);
    nextState(state,action);

end
end